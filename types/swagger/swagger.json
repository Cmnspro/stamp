        "summary": "Login with local user",
        "operationId": "PostLoginRoute",
        "responses": {
          "200": {
            "description": "PostLoginResponse",
            "schema": {
              "$ref": "#/definitions/PostLoginResponse"
            }
          },
          "401": {
            "description": "HTTPError",
            "schema": {
              "$ref": "#/definitions/HTTPError"
            }
          },
          "default": {
            "description": "HTTPError",
            "schema": {
              "$ref": "#/definitions/HTTPError"
            }
          }
        }
      }
    }
  },
  "definitions": {
